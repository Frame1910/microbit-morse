<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="main">
    <property name="height">250</property>
    <property name="padding">10</property>
    <property name="relief">flat</property>
    <property name="width">250</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Entry" id="entryText">
        <property name="font">TkFixedFont</property>
        <property name="justify">left</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Enter Desired Text</property>
        <property name="validate">none</property>
        <layout>
          <property name="column">0</property>
          <property name="pady">20</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Message" id="output">
        <property name="anchor">w</property>
        <property name="font">TkMenuFont</property>
        <property name="justify">center</property>
        <property name="relief">flat</property>
        <property name="text" translatable="yes">Output:</property>
        <bind add="" handler="" sequence="" />
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="run">
        <property name="command">self.translator</property>
        <property name="compound">top</property>
        <property name="default">normal</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Translate</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
